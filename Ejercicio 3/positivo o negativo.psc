Proceso Positivo_o_negativo
	Definir Num como real;
	escribir "escriba el numero que desea verificar";
	leer Num;
	si Num>0 Entonces
		escribir "el numero es positivo";
	SiNo
		si Num=0 Entonces
			escribir "el numero es igual a 0";
			sino escribir "el numero es negativo";
		FinSi
	FinSi
	
FinProceso
